  "amp": true,
  "epoch_lr_decay": 0.95,

  "lr_schedule": "const_with_warmup",
  "warmup_pct": 0.2,

  "init_lr": 1e-4,
  "init_bert_lr": 1e-5,

  "loss_func": "pairwise_margin",
  "weight_decay": 1e-7,

  "backprop_batch_size": 1,
  "batch_size": 16,
  "batch_size_val": 16,

  "max_query_val": 5000
}
